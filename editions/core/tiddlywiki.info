{
	"description": "Export TiddlyWiki core plugin",
	"plugins": [],
	"themes":[],
	"build": {
    "plain": [
			"--compress",
        "no",
      "--render",
        "$:/core",
        "[filenamify[]addprefix[./tiddlywiki/core/]addsuffix[.json-%BUILD_TIDDLYWIKI_CORE_VERSION%.json]]",
        "text/plain",
        "$:/core/templates/json-tiddler"
    ],
    "compressed": [
			"--compress",
        "yes",
      "--render",
        "$:/core",
        "[filenamify[]addprefix[./tiddlywiki/core/]addsuffix[.zlib-%BUILD_TIDDLYWIKI_CORE_VERSION%.json]]",
        "text/plain",
        "$:/core/templates/json-tiddler"
    ]
	}
}