{
	"description": "Build Ipfs TiddlyWiki plugin",
	"plugins": [],
	"themes":[],
	"build": {
    "plain": [
			"--compress",
        "no",
      "--render",
        "[tag[$:/ipfs/core]]",
        "[filenamify[]addprefix[./tiddlywiki-ipfs/plugin/]addsuffix[.js-%BUILD_PLUGIN_VERSION%.json]]",
        "text/plain",
        "$:/core/templates/json-tiddler"
    ],
    "compressed": [
			"--compress",
        "yes",
      "--render",
        "[tag[$:/ipfs/core]]",
        "[filenamify[]addprefix[./tiddlywiki-ipfs/plugin/]addsuffix[.js.zlib-%BUILD_PLUGIN_VERSION%.json]]",
        "text/plain",
        "$:/core/templates/json-tiddler"
    ]
	}
}