{
	"description": "Export Ipfs with TiddlyWiki plugin",
	"plugins": [],
	"themes":[],
	"build": {
    "plain": [
			"--compress",
        "no",
      "--render",
        "$:/plugins/ipfs",
        "[filenamify[]addprefix[./tiddlywiki-ipfs/plugin/]addsuffix[.json-%BUILD_PLUGIN_VERSION%.json]]",
        "text/plain",
        "$:/core/templates/json-tiddler"
    ],
    "compressed": [
			"--compress",
        "yes",
      "--render",
        "$:/plugins/ipfs",
        "[filenamify[]addprefix[./tiddlywiki-ipfs/plugin/]addsuffix[.zlib-%BUILD_PLUGIN_VERSION%.json]]",
        "text/plain",
        "$:/core/templates/json-tiddler"
    ]
	}
}