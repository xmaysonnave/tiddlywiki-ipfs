{
  "plugins": [],
  "includeWikis": [],
  "themes": [],
  "build": {
		"index": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.zlib.ipfs-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./build/output/editions/empty/$_ipfs_edition-build.tid",
        "application/x-tiddler",
      "--import",
        "./download/tiddlywiki/index.html",
        "text/html",
      "--import",
        "./build/tiddlers/systems/$_SplashScreen.tid",
        "application/x-tiddler",
      "--public-gateway",
        "%IPFS_PUBLIC_GATEWAY%",
			"--ipfs-modules",
        "yes",
			"--compress",
        "yes",
			"--rendertiddler",
        "$:/core/save/all",
        "./editions/tiddlywiki/tiddlywiki-%BUILD_TIDDLYWIKI_VERSION%.html",
        "text/plain"
    ]
  }
}
