{
  "plugins": [],
  "includeWikis": [],
  "themes": [],
  "build": {
		"index": [
      "--import",
        "./production/tiddlywiki-ipfs/sjcl/$_library_sjcl.js.json-%BUILD_SJCL_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js.json-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.json-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.json.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.zlib.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./build/output/editions/tiddlywiki/ipfs.edition.build.tid",
        "application/x-tiddler",
      "--import",
        "./download/tiddlywiki/index.html",
        "text/html",
      "--import",
        "./build/tiddlers/systems/$__SplashScreen.tid",
        "application/x-tiddler",
			"--ipfsmodules",
        "yes",
			"--compress",
        "yes",
			"--rendertiddler",
        "$:/core/save/all",
        "./editions/tiddlywiki/tiddlywiki-%BUILD_TIDDLYWIKI_VERSION%.html",
        "text/plain"
    ],
		"favicon": [
			"--savetiddler",
        "$:/favicon.ico",
        "./editions/tiddlywiki/favicon.ico"
    ]
  }
}
