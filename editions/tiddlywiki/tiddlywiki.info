{
  "plugins": [],
  "includeWikis": [],
  "themes": [],
  "build": {
		"index": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./download/tiddlywiki/index.html",
        "text/html",
      "--import",
        "./build/tiddlers/systems/$__SplashScreen.tid",
        "application/x-tiddler",
			"--compress",
        "yes",
			"--rendertiddler",
        "$:/core/save/all",
        "./editions/tiddlywiki/tiddlywiki-%BUILD_TIDDLYWIKI_VERSION%.html",
        "text/plain"
    ],
		"favicon": [
			"--savetiddler",
        "$:/favicon.ico",
        "./editions/tiddlywiki/favicon.ico"
    ]
  }
}
