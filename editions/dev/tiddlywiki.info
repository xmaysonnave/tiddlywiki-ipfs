{
  "plugins": [
		"tiddlywiki/browser-sniff",
		"tiddlywiki/evernote",
		"tiddlywiki/internals",
    "tiddlywiki/markdown",
    "tiddlywiki/menubar",
		"tiddlywiki/railroad"
  ],
  "includeWikis": [
  ],
  "themes": [
    "tiddlywiki/vanilla",
    "tiddlywiki/snowwhite"
  ],
  "build": {
		"dev": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
			"--compress",
        "yes",
      "--encryptionpublickey",
        "%DEV_DERIVED_PUBLIC_KEY%",
			"--rendertiddler",
        "$:/core/save/all",
        "./editions/dev/index.html",
        "text/plain",
      "--clearkeys"
    ],
		"dev-compressed": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
			"--compress",
        "yes",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/dev-compressed.html",
        "text/plain"
    ],
		"dev-standford": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
			"--compress",
        "yes",
      "--password",
        "blue",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/dev-standford.html",
        "text/plain",
      "--clearkeys"
    ],
    "dev-plain": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
			"--compress",
        "no",
      "--rendertiddler",
        "$:/core/save/all",
        "../sample/dev-plain.html",
        "text/plain"
    ],
    "dev-plain-and-ethereum": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
			"--compress",
        "no",
      "--encryptionpublickey",
        "%DEV_DERIVED_PUBLIC_KEY%",
      "--rendertiddler",
        "$:/core/save/all",
        "../sample/dev-plain-and-ethereum.html",
        "text/plain",
      "--clearkeys"
    ],
    "dev-plain-and-standford": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
			"--compress",
        "no",
      "--password",
        "blue",
      "--rendertiddler",
        "$:/core/save/all",
        "../sample/dev-plain-and-standford.html",
        "text/plain",
      "--clearkeys"
    ],
		"favicon": [
			"--savetiddler",
        "$:/favicon.ico",
        "../production/editions/dev/favicon.ico"
    ]
  }
}
