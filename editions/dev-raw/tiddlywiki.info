{
  "plugins": [
		"tiddlywiki/browser-sniff",
		"tiddlywiki/evernote",
		"tiddlywiki/internals",
    "tiddlywiki/markdown",
    "tiddlywiki/menubar",
		"tiddlywiki/railroad"
  ],
  "includeWikis": [
  ],
  "themes": [
    "tiddlywiki/vanilla",
    "tiddlywiki/snowwhite"
  ],
  "build": {
		"dev": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.zlib.ipfs-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.zlib.json-build.tid",
        "application/x-tiddler",
      "--public-gateway",
        "%IPFS_PUBLIC_GATEWAY%",
			"--ipfs-modules",
        "yes",
			"--compress",
        "yes",
      "--encryptionpublickey",
        "%DEV_DERIVED_PUBLIC_KEY%",
			"--rendertiddler",
        "$:/core/save/all",
        "./editions/dev/dev-%BUILD_DEV_VERSION%.html",
        "text/plain",
      "--clearkeys"
    ]
  }
}
