{
  "plugins": [
		"tiddlywiki/browser-sniff",
		"tiddlywiki/evernote",
		"tiddlywiki/internals",
    "tiddlywiki/markdown",
    "tiddlywiki/menubar",
		"tiddlywiki/railroad"
  ],
  "includeWikis": [
  ],
  "themes": [
    "tiddlywiki/vanilla",
    "tiddlywiki/snowwhite"
  ],
  "build": {
		"index": [
			"--compress", "yes",
			"--rendertiddler", "$:/core/save/all", "index.html", "text/plain"
    ],
		"index-standford": [
			"--compress", "yes",
      "--password", "blue",
			"--rendertiddler", "$:/core/save/all", "../sample/index-standford.html", "text/plain",
      "--clearpassword"
    ],
    "index-plain": [
      "--rendertiddler", "$:/core/save/all", "../sample/index-plain.html", "text/plain"
    ],
		"index-plain-and-ethereum": [
      "--encryptionpublickey", "6yzMxyMBxlKRxZSGaLuN3GEZkOaFjuejFGhYfKIRXWI=",
			"--rendertiddler", "$:/core/save/all", "../sample/index-plain-and-ethereum.html", "text/plain",
      "--clearencryptionpublickey"
    ],
		"index-plain-and-standford": [
      "--password", "blue",
			"--rendertiddler", "$:/core/save/all", "../sample/index-plain-and-standford.html", "text/plain",
      "--clearpassword"
    ]
  }
}
