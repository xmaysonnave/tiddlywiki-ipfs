{
	"description": "Build Ipfs TiddlyWiki documentation",
	"plugins": [],
	"themes":[],
	"build": {
    "plain": [
			"--compress",
        "no",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.json.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.zlib.json-build.tid",
        "application/x-tiddler",
      "--rendertiddler",
        "$:/core/templates/exporters/JsonFile",
        "./tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "text/plain",
        "",
        "exportFilter",
        "[tag[$:/ipfs/documentation]]"
    ],
    "compressed": [
			"--compress",
        "yes",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.json.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.zlib.json-build.tid",
        "application/x-tiddler",
      "--rendertiddler",
        "$:/core/templates/exporters/JsonFile",
        "./tiddlywiki-ipfs/documentation/$_ipfs_documentation.zlib-%BUILD_DOCUMENTATION_VERSION%.json",
        "text/plain",
        "",
        "exportFilter",
        "[tag[$:/ipfs/documentation]]"
    ]
	}
}