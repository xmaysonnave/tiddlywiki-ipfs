{
  "plugins": [
		"tiddlywiki/browser-sniff",
		"tiddlywiki/evernote",
		"tiddlywiki/internals",
    "tiddlywiki/markdown",
    "tiddlywiki/menubar",
		"tiddlywiki/railroad"
  ],
  "includeWikis": [
  ],
  "themes": [
    "tiddlywiki/vanilla",
    "tiddlywiki/snowwhite"
  ],
  "build": {
		"index": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-library-modules.js-%BUILD_LIBRARY_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.json-%BUILD_DOCUMENTATION_SEMVER%.json",
        "application/json",
			"--compress",
        "yes",
			"--rendertiddler",
        "$:/core/save/all",
        "./editions/bluelightav/index.html",
        "text/plain"
    ],
		"index-standford": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-library-modules.js-%BUILD_LIBRARY_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.json-%BUILD_DOCUMENTATION_SEMVER%.json",
        "application/json",
			"--compress",
        "yes",
      "--password",
        "blue",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/index-standford.html",
        "text/plain",
      "--clearpassword"
    ],
    "index-plain": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-library-modules.js-%BUILD_LIBRARY_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.json-%BUILD_DOCUMENTATION_SEMVER%.json",
        "application/json",
      "--rendertiddler",
        "$:/core/save/all",
        "../sample/index-plain.html",
        "text/plain"
    ],
		"index-plain-and-ethereum": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-library-modules.js-%BUILD_LIBRARY_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.json-%BUILD_DOCUMENTATION_SEMVER%.json",
        "application/json",
      "--encryptionpublickey",
        "6yzMxyMBxlKRxZSGaLuN3GEZkOaFjuejFGhYfKIRXWI=",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/index-plain-and-ethereum.html",
        "text/plain",
      "--clearencryptionpublickey"
    ],
		"index-plain-and-standford": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-library-modules.js-%BUILD_LIBRARY_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_SEMVER%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.json-%BUILD_DOCUMENTATION_SEMVER%.json",
        "application/json",
      "--password",
        "blue",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/index-plain-and-standford.html",
        "text/plain",
      "--clearpassword"
    ],
		"prod_favicon": [
			"--savetiddler",
        "$:/favicon.ico",
        "../production/editions/bluelightav/favicon.png"
    ],
		"sample_favicon": [
			"--savetiddler",
        "$:/favicon.ico",
        "../sample/favicon.png"
    ]
  }
}
