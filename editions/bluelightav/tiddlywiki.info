{
  "plugins": [
		"tiddlywiki/browser-sniff",
		"tiddlywiki/evernote",
		"tiddlywiki/internals",
    "tiddlywiki/markdown",
    "tiddlywiki/menubar",
		"tiddlywiki/railroad"
  ],
  "includeWikis": [
  ],
  "themes": [
    "tiddlywiki/vanilla",
    "tiddlywiki/snowwhite"
  ],
  "build": {
		"index": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
      "--import",
        "./build/output/editions/bluelightav/ipfs.edition.build.tid",
        "application/x-tiddler",
			"--compress",
        "yes",
			"--rendertiddler",
        "$:/core/save/all",
        "./editions/bluelightav/bluelightav-%BUILD_BLUELIGHTAV_VERSION%.html",
        "text/plain"
    ],
		"index-standford": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
      "--import",
        "./build/output/editions/bluelightav/ipfs.edition.build.tid",
        "application/x-tiddler",
			"--compress",
        "yes",
      "--password",
        "blue",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/index-standford.html",
        "text/plain",
      "--clearkeys"
    ],
    "index-plain": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
      "--import",
        "./build/output/editions/bluelightav/ipfs.edition.build.tid",
        "application/x-tiddler",
			"--compress",
        "no",
      "--rendertiddler",
        "$:/core/save/all",
        "../sample/index-plain.html",
        "text/plain"
    ],
		"index-plain-and-ethereum": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
      "--import",
        "./build/output/editions/bluelightav/ipfs.edition.build.tid",
        "application/x-tiddler",
			"--compress",
        "no",
      "--encryptionpublickey",
        "%DEV_DERIVED_PUBLIC_KEY%",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/index-plain-and-ethereum.html",
        "text/plain",
      "--clearkeys"
    ],
		"index-plain-and-standford": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
      "--import",
        "./build/output/editions/bluelightav/ipfs.edition.build.tid",
        "application/x-tiddler",
			"--compress",
        "no",
      "--password",
        "blue",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/index-plain-and-standford.html",
        "text/plain",
      "--clearkeys"
    ],
		"prod_favicon": [
			"--savetiddler",
        "$:/favicon.ico",
        "../production/editions/bluelightav/favicon.ico"
    ],
		"sample_favicon": [
			"--savetiddler",
        "$:/favicon.ico",
        "../sample/favicon.ico"
    ]
  }
}
