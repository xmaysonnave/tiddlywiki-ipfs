{
  "plugins": [
		"tiddlywiki/browser-sniff",
		"tiddlywiki/evernote",
		"tiddlywiki/internals",
    "tiddlywiki/markdown",
    "tiddlywiki/menubar",
		"tiddlywiki/railroad"
  ],
  "includeWikis": [
  ],
  "themes": [
    "tiddlywiki/vanilla",
    "tiddlywiki/snowwhite"
  ],
  "build": {
		"index": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.zlib.ipfs-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.zlib.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./build/output/editions/bluelightav/$_ipfs_edition-build.tid",
        "application/x-tiddler",
      "--public-gateway",
        "%IPFS_PUBLIC_GATEWAY%",
			"--tree-layout",
        "yes",
			"--compress",
        "yes",
			"--rendertiddler",
        "$:/core/save/all",
        "./editions/bluelightav/bluelightav-%BUILD_BLUELIGHTAV_VERSION%.html",
        "text/plain"
    ],
		"index-embedded-standford": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.json-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.zlib.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./build/output/editions/bluelightav/$_ipfs_edition-build.tid",
        "application/x-tiddler",
			"--tree-layout",
        "no",
			"--compress",
        "yes",
      "--password",
        "blue",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/index-embedded-standford.html",
        "text/plain",
      "--clearkeys"
    ],
    "index-embedded-plain": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.json-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.zlib.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./build/output/editions/bluelightav/$_ipfs_edition-build.tid",
        "application/x-tiddler",
			"--tree-layout",
        "no",
			"--compress",
        "no",
      "--rendertiddler",
        "$:/core/save/all",
        "../sample/index-embedded-plain.html",
        "text/plain"
    ],
		"index-embedded-plain-ethereum": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.json-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.zlib.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./build/output/editions/bluelightav/$_ipfs_edition-build.tid",
        "application/x-tiddler",
			"--tree-layout",
        "no",
			"--compress",
        "no",
      "--encryptionpublickey",
        "%DEV_DERIVED_PUBLIC_KEY%",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/index-plain-ethereum.html",
        "text/plain",
      "--clearkeys"
    ],
		"index-embedded-plain-standford": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.json-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation.zlib.json-build.tid",
        "application/x-tiddler",
      "--import",
        "./build/output/editions/bluelightav/$_ipfs_edition-build.tid",
        "application/x-tiddler",
			"--tree-layout",
        "no",
			"--compress",
        "no",
      "--password",
        "blue",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/index-embedded-plain-standford.html",
        "text/plain",
      "--clearkeys"
    ]
  }
}
