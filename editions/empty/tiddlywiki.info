{
	"description": "Empty edition",
	"plugins": [
	],
	"themes": [
		"tiddlywiki/vanilla",
		"tiddlywiki/snowwhite"
	],
	"build": {
		"empty": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
			"--compress",
        "yes",
			"--rendertiddler",
        "$:/core/save/all",
        "./editions/empty/empty-%BUILD_EMPTY_VERSION%.html",
        "text/plain"
		],
		"empty-ethereum": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
			"--compress",
        "yes",
      "--encryptionpublickey",
        "%DEV_DERIVED_PUBLIC_KEY%",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/empty-ethereum.html",
        "text/plain",
      "--clearkeys"
    ],
		"empty-standford": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
			"--compress",
        "yes",
      "--password",
        "blue",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/empty-standford.html",
        "text/plain",
      "--clearkeys"
    ],
		"empty-plain": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
			"--compress",
        "no",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/empty-plain.html",
        "text/plain"
		],
		"empty-plain-and-ethereum": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
			"--compress",
        "no",
      "--encryptionpublickey",
        "%DEV_DERIVED_PUBLIC_KEY%",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/empty-plain-and-ethereum.html",
        "text/plain",
      "--clearkeys"
    ],
		"empty-plain-and-standford": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.js-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
			"--compress",
        "no",
      "--password",
        "blue",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/empty-plain-and-standford.html",
        "text/plain",
      "--clearkeys"
    ],
		"favicon": [
			"--savetiddler",
        "$:/favicon.ico",
        "../production/editions/empty/favicon.ico"
    ]
	}
}