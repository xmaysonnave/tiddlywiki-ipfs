{
	"description": "Empty edition",
	"plugins": [
	],
	"themes": [
		"tiddlywiki/vanilla",
		"tiddlywiki/snowwhite"
	],
	"build": {
		"empty": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.zlib.ipfs-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./build/output/editions/empty/$_ipfs_edition-build.tid",
        "application/x-tiddler",
			"--remote-layout",
        "yes",
			"--compress",
        "yes",
			"--rendertiddler",
        "$:/core/save/all",
        "./editions/empty/empty-%BUILD_EMPTY_VERSION%.html",
        "text/plain"
		],
		"empty-remote-plain": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.zlib.ipfs-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./build/output/editions/empty/$_ipfs_edition-build.tid",
        "application/x-tiddler",
			"--remote-layout",
        "yes",
			"--compress",
        "no",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/empty-remote-plain.html",
        "text/plain"
		],
		"empty-embedded-plain": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.zlib.ipfs-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./build/output/editions/empty/$_ipfs_edition-build.tid",
        "application/x-tiddler",
			"--remote-layout",
        "no",
			"--compress",
        "no",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/empty-embedded-plain.html",
        "text/plain"
		],
		"empty-remote-ethereum": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.json-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./build/output/editions/empty/$_ipfs_edition-build.tid",
        "application/x-tiddler",
			"--remote-layout",
        "yes",
			"--compress",
        "yes",
      "--encryptionpublickey",
        "%ETHEREUM_DERIVED_PUBLIC_KEY%",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/empty-remote-ethereum.html",
        "text/plain",
      "--clearkeys"
    ],
		"empty-embedded-standford": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.json-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./build/output/editions/empty/$_ipfs_edition-build.tid",
        "application/x-tiddler",
			"--remote-layout",
        "no",
			"--compress",
        "yes",
      "--password",
        "blue",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/empty-embedded-standford.html",
        "text/plain",
      "--clearkeys"
    ],
		"empty-embedded-plain-ethereum": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.json-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./build/output/editions/empty/$_ipfs_edition-build.tid",
        "application/x-tiddler",
			"--remote-layout",
        "no",
			"--compress",
        "no",
      "--encryptionpublickey",
        "%ETHEREUM_DERIVED_PUBLIC_KEY%",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/empty-embedded-plain-ethereum.html",
        "text/plain",
      "--clearkeys"
    ],
		"empty-embedded-plain-standford": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.json-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./build/output/editions/empty/$_ipfs_edition-build.tid",
        "application/x-tiddler",
			"--remote-layout",
        "no",
			"--compress",
        "no",
      "--password",
        "blue",
			"--rendertiddler",
        "$:/core/save/all",
        "../sample/empty-embedded-plain-standford.html",
        "text/plain",
      "--clearkeys"
    ]
	}
}