{
  "plugins": [
		"tiddlywiki/browser-sniff",
		"tiddlywiki/evernote",
		"tiddlywiki/internals",
    "tiddlywiki/markdown",
    "tiddlywiki/menubar",
		"tiddlywiki/railroad"
  ],
  "includeWikis": [
  ],
  "themes": [
    "tiddlywiki/vanilla",
    "tiddlywiki/snowwhite"
  ],
  "build": {
		"index": [
      "--import",
        "./production/tiddlywiki-ipfs/library/$_library_ipfs-modules.js.json-%BUILD_LIBRARY_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/boot/$_boot_boot.js.json-%BUILD_BOOT_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/plugin/$_plugins_ipfs.json-%BUILD_PLUGIN_VERSION%.json",
        "application/json",
      "--import",
        "./production/tiddlywiki-ipfs/documentation/$_ipfs_documentation-%BUILD_DOCUMENTATION_VERSION%.json",
        "application/json",
			"--compress",
        "yes",
			"--rendertiddler",
        "$:/core/save/all",
        "./editions/bluelightav/bluelightav-%BUILD_BLUELIGHTAV_VERSION%.html",
        "text/plain"
    ]
  }
}
